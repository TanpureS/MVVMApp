# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'FacebookPOC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FacebookPOC
  pod 'FBSDKCoreKit', '~> 4.21'
  pod 'FBSDKLoginKit', '~> 4.21'
  pod 'FBSDKShareKit', '~> 4.21'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'MBProgressHUD', '~> 1.1.0'

  target 'FacebookPOCTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FacebookPOCUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
